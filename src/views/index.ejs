<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Registro</title>
    <%- include("partials/head") -%>
</head>

<body>

    <%- include("partials/navigation") %>

                <% images.forEach(function (dato) { %>
                    <table class="table table-striped table-hover">
                        <thead>
                            <tr>
                                <th>Nombre</th>
                                <th>Apellidos</th>
                                <th>Telefono</th>
                                <th>Direccion</th>
                                <th>Bachillerato de procedencia</th>
                                <th>Imagen</th>
                                <th>Acciones</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td scope="row"><%= dato.nombre %></td>
                                <td><%= dato.apePat %> <%= dato.apeMat %></td>
                                <td><%= dato.telefonoPersonal %></td>
                                <td><%= dato.calle %>, NO. <%= dato.noExterior %> , <%= dato.colonia %>, <%= dato.ciudad %> </td>
                                <td><%= dato.bachilleratoPro %></td>
                                <td><img width="70" src="<%= dato.path %>" alt="<%= dato.filename %>"></td>
                                <td><a href="/registroDocumento/<%= dato._id %>" class="btn btn-primary">
                                    View 
                                 </a></td>
                            </tr>
                        </tbody>
                </table>
                
                <% }) %>

</body>

</html>