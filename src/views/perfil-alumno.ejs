<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Perfil completo</title>
    <link rel="icon" href="/img/registro.png">
    <%- include("partials/head") -%>
</head>

<body>

    <%- include("partials/navigation") %>

        <div class="container py-5 h-100">
          <div class="row d-flex justify-content-center align-items-center h-100">
            <div class="col-xl-10">
              <div class="card rounded-3 text-black">
                <div class="container">
                  <div class="card-body p-md-5 mx-md-4">
                    <h1 style="text-align: center;"><strong> DATOS DEL ALUMNOS</strong></h1> 
                        
                        <% datoRegistro.forEach(function (item) { %>
                            <form style="margin-top: 2rem;" class="row g-3" action="/" method="get" enctype="multipart/form-data">

                                <div class="col-6" style="text-align: center;">
                                    <h6 style="margin-top: 1rem; text-align: center;"><strong>Foto de perfil</strong></h6>
                                    <img src="/uploads/<%= item.fotoEstudiante %>" alt=""  width="200">
                                </div>


                                <div class="col-6">
                                  <label for="nombre" class="form-label">Nombre(s).</label>
                                  <input type="text" name="nombre" disabled value="<%= item.nombre %>" placeholder="nombre" class="form-control">
                                </div>
                                
                              <div class="col-md-6">
                                <label for="apePat" class="form-label">Apellido paterno.</label>
                                <input type="text" name="apePat" disabled value="<%= item.apePat %>" placeholder="apellido paterno" class="form-control">
                              </div>
                    
                              <div class="col-6">
                                <label for="apeMat" class="form-label">Apellido materno.</label>
                                <input type="text" name="apeMat" disabled value="<%= item.apeMat %>" placeholder="apellido materno" class="form-control">
                              </div>
              
                              <div class="col-6">
                                <label for="correo" class="form-label">Email</label>
                                <input type="email" name="correo" disabled value="<%= item.correo %>" placeholder="correo electronico" class="form-control">
                              </div>
                              
                              <div style="margin-top: 1rem;" class="row">
                                <div class="mb-4 col-4">
                                  <label for="telefonoPersonal" class="form-label">Telefono personal.</label>
                                  <input type="tel" name="telefonoPersonal" disabled value="<%= item.telefonoPersonal %>" placeholder="Telefono personal"   maxlength="10" class="form-control">
                                </div>
                
                                <div class="mb-4 col-4">
                                  <label for="telefonoCasa" class="form-label">Telefono de tu domicilio.</label>
                                  <input type="tel" name="telefonoCasa" disabled value="<%= item.telefonoCasa%>" placeholder="Telefono de tu domicilio"  maxlength="10" class="form-control">
                                </div>
                
                                <div class="mb-4 col-4">
                                  <label for="telefonoTutor" class="form-label">Telefono del tutor acargo.</label>
                                  <input type="tel" name="telefonoTutor" disabled value="<%= item.telefonoTutor %>"  placeholder="Telefono del tutor"  maxlength="10" class="form-control">
                                </div>
                  
                              </div>
              
                              <div class="container">
              
                                <h4 style="margin-top: 3rem;">Direccion de tu hogar</h4>
                              </div>
                          
              
                              <div class="col-md-6">
                                <label for="calle" class="form-label">Calle.</label>
                                <input type="text" name="calle" disabled value="<%= item.calle %>" placeholder="calle" class="form-control">
                              </div>
                    
                              <div class="col-6">
                                <label for="noExterior" class="form-label">Numero exterior</label>
                                <input type="number" name="noExterior" disabled value="<%= item.noExterior %>" placeholder="noExterior" class="form-control">                       
                              </div>
              
                              <div class="col-6">
                                <label for="colonia" class="form-label">Colonia</label>
                                <input type="text" name="colonia" placeholder="colonia" disabled value="<%= item.colonia %>" class="form-control">
                              </div>
              
                              <div class="col-6">
                                <label for="ciudad" class="form-label">Ciudad</label>
                                <input type="text" name="ciudad" placeholder="ciudad" disabled value="<%= item.ciudad %>" class="form-control">
                              </div>
                                
                              <div class="col-6">
                                <label for="cP" class="form-label">Codigo postal</label>
                                <input type="tel" name="cP" placeholder="codigo postal" disabled value="<%= item.cP %>" maxlength="5" class="form-control">
                              </div>
              
                              <div class="container">
              
                                <h4 style="margin-top: 3rem;">Informacion Escolar</h4>
                              </div>
              
                              <div class="col-6">
                                <label for="bachilleratoPro" class="form-label">Bachillerato de procedencia</label>
                                <input type="text" name="bachilleratoPro" disabled value="<%= item.bachilleratoPro %>" placeholder="Bachillerato de procedencia" class="form-control"> 
                              </div>
                              
                              <div class="col-6">
                                <label for="promedio" class="form-label">Promedio</label>
                                <input type="number" name="promedio" disabled value="<%= item.promedio%>" maxlength="1" placeholder="Ultimo promedio obtenido" class="form-control">
                              </div>
                              
                              <div class="col-6">
                                <label for="especialidadCursada" class="form-label">Especialidad cursada</label>
                                <input type="text" name="especialidadCursada" disabled value="<%= item.especialidadCursada %>" placeholder="Especialidad cursada" class="form-control">
                              </div>
              
                              <div class="container">
              
                                <h4 style="margin-top: 3rem;">Informacion de los padres</h4>
                              </div>
              
                              <div class="container">
                                <h6>Mama</h6>
                              </div>
              
                              <div class="col-6">
                                <label for="nombreMama" class="form-label">Nombre de la madre</label>
                                <input type="text" name="nombreMama" disabled value="<%= item.nombreMama %>" placeholder="nombre de mama" class="form-control">
                              </div>
                              
                              <div class="col-6">
                                <label for="apePatMama" class="form-label">Apellido paterno de la madre</label>
                                <input type="text" name="apePatMama" disabled value="<%= item.apePatMama %>" placeholder="apellido de paterno de mama" class="form-control">
                              </div>
              
                              <div class="col-6">
                                <label for="apeMatMama" class="form-label">Apellido materno de la madre</label>
                                <input type="text" name="apeMatMama" disabled value="<%= item.apeMatMama %>" placeholder="apellido de materno de mama" class="form-control">
                              </div>
              
                              <div class="container">
                                <h6 style="margin-top: 1rem;">Papa</h6>
                              </div>
              
                              <div class="col-6">
                                <label for="nombreMama" class="form-label">Nombre del padre</label>
                                <input type="text" name="nombrePapa" disabled value="<%= item.nombrePapa %>" placeholder="nombre del padre" class="form-control">
                              </div>
                              
                              <div class="col-6">
                                <label for="apePatPapa" class="form-label">Apellido paterno del padre</label>
                                <input type="text" name="apePatPapa" disabled value="<%= item.apePatPapa %>" placeholder="apellido de paterno de mama" class="form-control">
                              </div>
              
                              <div class="col-6">
                                <label for="apeMatPapa" class="form-label">Apellido materno del padre</label>
                                <input type="text" name="apeMatPapa" disabled value="<%= item.apeMatPapa %>" placeholder="apellido de materno de mama" class="form-control">
                              </div>
                              
                              <div class="col-6">

                              </div>
                       
                              <div class="col-6" style="text-align: center;">
                                <h6 style="margin-top: 1rem; text-align: center;"><strong>Certificado de bachillerato</strong></h6>
                                <a href="/uploads/<%= item.certificado %>" target="_blank"><svg xmlns="http://www.w3.org/2000/svg" width="100" height="100" fill="currentColor"  style="color: black;" class="bi bi-file-earmark-pdf" viewBox="0 0 16 16">
